{
  "name": "express-ts-prisma-starter",
  "version": "1.0.2",
  "description": "Express TypeScript Boilerplate with Authentication and Authorization using Prisma ORM",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "authentication",
    "authorization"
  ],
  "author": "Ramdhan Rizki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramdhanrizkij/express-prisma-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ramdhanrizkij/express-prisma-typescript-starter/issues"
  },
  "bin": {
    "create-express-ts-app": "./bin/cli.js"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/morgan": "^1.9.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
